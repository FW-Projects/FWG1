                else if (sFWG2_t.general_parameter.code_mode_state == CODE_MODE_STOP)
                {
                    sFWG2_t.general_parameter.code_mode_step = CODE_WAIT;
                }

                break;
            }
        }

        dwim->read_size = 0;
    }

